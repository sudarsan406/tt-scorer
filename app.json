    "icon": "./assets/tt-scorer-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/tt-scorer-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a2332"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.skanagala.ttscorer",
