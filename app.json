    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tt-scorer-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.skanagala.ttscorer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "22d038dc-1f57-4a70-8115-91a01c529b6d"
      }
    },
    "owner": "sudarsan406"
  }
