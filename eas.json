{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "env": {
        "NODE_ENV": "production"
      },
      "node": "20.18.1",
      "cache": {
        "key": "production-cache",
        "disabled": false,
        "paths": ["node_modules"]
      },
      "ios": {
        "distribution": "store",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "sudarsan406@gmail.com",
        "ascAppId": "6752398902"
      }
    }
  }
}